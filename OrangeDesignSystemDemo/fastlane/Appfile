apple_id(ENV["ODS_FASTLANE_APPLE_ID"])
team_id(ENV["ODS_DEVELOPER_PORTAL_TEAM_ID"])

for_lane :qualif do
  app_identifier(ENV["ODS_DEVELOPER_APP_IDENTIFIER_QUALIF"])
  ENV["TESTFLIGHT_GROUPS"] =  "qualif-team"
end

for_lane :prod do
  app_identifier(ENV["ODS_DEVELOPER_APP_IDENTIFIER_PROD"])
  ENV["TESTFLIGHT_GROUPS"] =  "public-access"
end
